   "source": [
    "from math import sin\n",
    "\n",
    "def flint_hills_gen(N):\n",
    "    for n in range(1, N):\n",
    "        denom = n * (n * sin(n)) ** 2\n",
    "        yield 1 / denom\n",
    "\n",
    "FH = flint_hills_gen(1000000)\n",
    "print(sum(FH))"
   ]
